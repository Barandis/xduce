{
  "name": "xduce",
  "version": "0.8.2",
  "description": "Composable algorithmic transformations library for JavaScript",
  "main": "dist/xduce.js",
  "scripts": {
    "cmt": "git-cz",
    "precommit": "npm test",
    "prepush": "npm test",
    "clean": "rimraf dist",
    "clean:docs": "rimraf docs/api",
    "build:es5": "webpack --config=webpack.config.js",
    "build:es5:min": "webpack --config=webpack.min.config.js",
    "build:es6": "webpack --config=webpack.es.config.js",
    "build:es6:min": "webpack --config=webpack.es.min.config.js",
    "build": "npm run clean && npm run build:es5 && npm run build:es5:min && npm run build:es6 && npm run build:es6:min",
    "docs": "node_modules/.bin/jsdoc -c ./jsdoc.json",
    "lint": "eslint ./src",
    "test": "mocha --recursive",
    "test:cover": "nyc mocha --recursive",
    "test:cover:upload": "npm run test:cover -s && cat ./coverage/lcov.info | coveralls",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "transducer",
    "transformer",
    "csp",
    "functional"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Barandis/xduce.git"
  },
  "bugs": {
    "url": "https://github.com/Barandis/xduce/issues"
  },
  "author": "Thomas Otterson <tjotterson@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "commitizen": "^2.9.6",
    "coveralls": "^2.12.0",
    "cz-conventional-changelog": "^2.0.0",
    "docdash": "^0.4.0",
    "eslint": "^3.17.0",
    "eslint-plugin-no-class": "^0.1.0",
    "husky": "^0.14.3",
    "immutable": "^3.8.1",
    "jsdoc": "^3.5.4",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
