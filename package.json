{
  "name": "xduce",
  "version": "0.8.1",
  "description": "Composable algorithmic transformations library for JavaScript",
  "main": "lib/xduce.js",
  "scripts": {
    "clean": "rimraf lib",
    "build:dev": "webpack",
    "build:prod": "webpack --config=webpack.min.config.js",
    "build": "npm run clean && npm run build:dev && npm run build:prod",
    "lint": "eslint ./src",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:cover": "nyc mocha --recursive",
    "test:cover:upload": "npm run test:cover -s && cat ./coverage/lcov.info | coveralls",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "transducer",
    "transformer",
    "csp",
    "functional"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Barandis/xduce.git"
  },
  "bugs": {
    "url": "https://github.com/Barandis/xduce/issues"
  },
  "author": "Thomas Otterson <tjotterson@gmail.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "eslint": "^3.17.0",
    "eslint-plugin-no-class": "^0.1.0",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {}
}
